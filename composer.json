{
  "name": "railroad/resora",
  "type": "library",
  "description": "A database access system.",
  "keywords": [
    "resora",
    "database"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "mockery/mockery": "^1.4.4",
    "fakerphp/faker": "^1.9.1",
    "orchestra/testbench": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Resora\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Resora\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
